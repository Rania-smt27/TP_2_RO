 int np = 8;
 
 range I = 1..np;
 range J = 1..np;
 
 int c[I][J] = [[0,30,0,24,25,0,0,0],
				[30,0,27,28,0,27,0,0],
			    [0,27,0,0,0,26,30,0],
				[24,28,0,0,29,21,0,0],
				[25,0,0,29,0,30,0,30],
				[0,27,26,21,30,0,26,36],
				[0,0,30,0,0,28,0,25],
				[0,0,0,0,30,36,25,0]];
 
 dvar boolean x[I][J];
 
 
 maximize sum(i in I,j in J) c[i][j]*x[i][j];
 
 subject to
 {
 sum(i in I,j in J)  x[i][j] <=1;
 }